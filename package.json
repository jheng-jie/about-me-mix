{
  "name": "about-me-mix",
  "version": "1.0.1",
  "description": "vue, react, svelte SSG(Static Site Generation) experiment",
  "license": "MIT",
  "private": true,
  "scripts": {
    "nuxt3": "yarn workspace @about-me-mix/nuxt3 dev",
    "svelte-kit2": "yarn workspace @about-me-mix/svelte-kit2 dev",
    "next14": "yarn workspace @about-me-mix/next14 dev",
    "angular17": "yarn workspace @about-me-mix/angular17 dev",
    "dev": "sh ./shell/dev.sh",
    "build": "sh ./shell/build.sh",
    "deploy": "rm -rf ./docs && cp -a ./about-me-mix ./docs",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "workspaces": [
    "projects/*"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "@unocss/postcss": "^0.57.5",
    "express": "^4.18.2",
    "gsap": "^3.11.4",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "twgl.js": "^5.3.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.5"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2"
  }
}
